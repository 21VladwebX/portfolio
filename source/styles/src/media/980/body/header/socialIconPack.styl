.socialIconPack
  width: 40%;
  top: 15%;
  left: 45%;
  z-index: 2;

  &__header
    width: 100%;
    height: 40px;
    letter-spacing: -1px;



    &-Brownsocial,&-other
      font-size: 1.5em;
      top: 25%;
      margin-left: -5%;

    &-Brownsocial
      left: 10%;

    &-other
      left: 28%;

    &-version
      top: 10%;
      left: 70%;
      font-size: 0.4em;

    &-size
      font-size: 0.5em;
      width: 8%;
      top: 15%;
      left: 86%;

    &-Brownsocial
      left: 8%;


  &Body__row1, &Body__row2
    display: grid;
    height: 45px;
    grid-template-rows: 50px 50px;
    grid-template-columns: repeat(6,55px);
    grid-row-gap: 2px;
    justify-content: space-between;
    grid-column-gap: 0px;
    img
      width: 80%;
      height: 80%;
